;\n                break;\n            case \"T\":\n                Object.assign(schedule, {\"t\": event.target.value});\n                break;\n            case \"D\":\n                Object.assign(schedule, {\"d\": event.target.value});\n                break;\n            case \"I\":\n                Object.assign(schedule, {\"i\": event.target.value});\n                break;\n            case \"S\":\n                Object.assign(schedule, {\"s\": event.target.value});\n                break;\n            default:\n                break;\n        }\n        \n        setSchedule(schedule);\n    }\n\n    const handleSubmit = () => {\n        const db = getDatabase(firebase);\n        const starCountRef = ref(db, 'schedules/' + computerId + \"/\" + id);\n        set(starCountRef, schedule);\n        alert(\"Cập nhật thành công.\");\n    }\n\n    return (\n        <Form method=\"post\">\n            <h1 className=\"text-center\">Thời gian biểu</h1>\n            <FormText>\n                <h3 className=\"text-center\">Lịch sử dụng máy tính trong ngày</h3>\n            </FormText>\n            \n            <FormGroup>\n                <Label for=\"F\">\n                Giờ bắt đầu\n                </Label>\n                <Input\n                id=\"F\"\n                name=\"f\"\n                placeholder=\"datetime placeholder\"\n                type=\"time\"\n                defaultValue={schedule.f}\n                onChange={handleInputChange}\n                />\n            </FormGroup>\n\n            <FormGroup>\n                <Label for=\"T\">\n                Giờ kết thúc\n                </Label>\n                <Input\n                id=\"T\"\n                name=\"t\"\n                placeholder=\"datetime placeholder\"\n                type=\"time\"\n                defaultValue={schedule.t}\n                onChange={handleInputChange}\n                />\n            </FormGroup>\n\n\n            <FormGroup>\n                <Label for=\"D\">\n                Thời gian sử dụng\n                </Label>\n                <Input\n                id=\"D\"\n                name=\"d\"\n                placeholder=\"0\"\n                type=\"number\"\n                defaultValue={schedule.d}\n                onChange={handleInputChange}\n                />\n            </FormGroup>\n\n\n            <FormGroup>\n                <Label for=\"I\">\n                Thời gian nghỉ\n                </Label>\n                <Input\n                id=\"I\"\n                name=\"i\"\n                placeholder=\"0\"\n                type=\"number\"\n                defaultValue={schedule.i}\n                onChange={handleInputChange}\n                />\n            </FormGroup>\n\n\n            <FormGroup>\n                <Label for=\"S\">\n                Tổng thời gian\n                </Label>\n                <Input\n                id=\"S\"\n                name=\"s\"\n                placeholder=\"0\"\n                type=\"number\"\n                defaultValue={schedule.s}\n                onChange={handleInputChange}\n                />\n            </FormGroup>\n            \n            <FormGroup>\n                <Button className=\"mr-5 btn btn-warning\" onClick={() => handleSubmit()}>Cập nhật</Button>\n                <input type=\"reset\" className=\"btn btn-danger\"/>\n            </FormGroup>\n        </Form>\n    )\n}\n\nexport default ScheduleContainer;","/home/zuy/Documents/process-memory-p/src/containers/AddScheduleContainer.js",[],"/home/zuy/Documents/process-memory-p/src/pages/AddSchedulePage.js",[],"/home/zuy/Documents/process-memory-p/src/pages/ImagePage.js",[],"/home/zuy/Documents/process-memory-p/src/containers/ImageContainer.js",["92","93"],"import React, { Fragment, useEffect, useState } from \"react\";\nimport { Table, Button } from \"reactstrap\";\n\nimport { getStorage, ref, listAll, deleteObject } from \"firebase/storage\";\nimport firebase from \"../firebase\";\n\nfunction ImageContainer() {\n    const [folders, setFolders] = useState([]);\n    const [images, setImages] = useState([]);\n    const [computerId, setComputerId] = useState(null);\n\n    useEffect(() => {\n        fetchData();\n    }, []);\n\n    async function fetchData() {\n        const login = JSON.parse(localStorage.getItem('login'));\n        const computerId = login.computerId;\n        setComputerId(computerId);\n\n        const db = getStorage(firebase, \"gs://process-memory-management.appspot.com\");\n        const imagesRef = ref(db, 'images/' + computerId);\n        let folderPaths = [];\n        let imagePaths = [];\n        await listAll(imagesRef)\n            .then((res) => {\n                res.prefixes.forEach((folderRef) => {\n                    folderPaths.push(folderRef);\n                });\n\n                res.items.forEach((itemRef) => {\n                    imagePaths.push(itemRef);\n                });\n            }).catch((error) => {\n                console.log(error);\n            });\n        setFolders(folderPaths);\n        setImages(imagePaths);\n    }\n\n    const handleDelete = (path) => {\n        console.log(path);\n        const db = getStorage(firebase, \"gs://process-memory-management.appspot.com\");\n        const imagesRef = ref(db, path);\n        listAll(imagesRef)\n            .then((res) => {\n                res.items.forEach((itemRef) => {\n                    deleteObject(ref(db, itemRef._location.path));\n                });\n            }).catch((error) => {\n                console.log(error);\n            });\n    }\n\n    //setInterval(fetchData, 5000);\n\n    return (\n        <Fragment>\n            <h1 className=\"text-center \">Nhật ký ảnh</h1>\n\n            <Table responsive>\n                <thead>\n                    <tr>\n                        <th colSpan={1}>ID</th>\n                        <th>\n                            Ngày\n                        </th>\n                        <th>Hosting</th>\n                        <th>Đường dẫn</th>\n                        <th>#</th>\n                    </tr>\n                </thead>\n\n                <tbody>\n                    {folders && folders.map((folder, index) => (\n                        <tr key={index} >\n                            <th scope=\"row\" colSpan={1}>\n                                {index}\n                            </th>\n                            <td >\n                                <a href={folder._location.path}>{folder.name}</a>\n                            </td>\n                            <td>\n                                {folder.bucket}\n                            </td>\n                            <td>{folder._location.path}</td>\n                            <td>\n                                <a href={folder._location.path} className=\"btn btn-primary\">Xem</a>\n                                <Button className=\"ml-5 btn btn-danger\" onClick={() => handleDelete(folder._location.path)}>Xóa</Button>\n                            </td>\n                        </tr>\n                    ))}\n\n                    {images && images.map((image, index) => (\n                        <tr key={index} >\n                            <th scope=\"row\" colSpan={1}>\n                                {index}\n                            </th>\n                            <td >\n                                <a href={image._location.path}>{image.name}</a>\n                            </td>\n                            <td>\n                                {image.bucket}\n                            </td>\n                            <td>{image._location.path}</td>\n                            <td>\n                                <a href={\"\"} className=\"btn btn-primary\">Xem</a>\n                                <Button className=\"ml-5 btn btn-danger\" onClick={() => handleDelete(image._location.path)}>Xóa</Button>\n                            </td>\n                        </tr>\n                    ))}\n\n                </tbody>\n            </Table>\n        </Fragment>\n    )\n}\n\nexport default ImageContainer;","/home/zuy/Documents/process-memory-p/src/pages/ImageGridPage.js",[],"/home/zuy/Documents/process-memory-p/src/containers/ImageGridContainer.js",["94","95","96","97","98"],"/home/zuy/Documents/process-memory-p/src/components/ImageGrid.js",["99"],"/home/zuy/Documents/process-memory-p/src/components/Modal.js",[],{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":20,"column":8,"nodeType":"106","endLine":20,"endColumn":10,"suggestions":"107"},{"ruleId":"108","severity":1,"message":"109","line":10,"column":12,"nodeType":"110","messageId":"111","endLine":10,"endColumn":22},{"ruleId":"112","severity":1,"message":"113","line":107,"column":33,"nodeType":"114","endLine":107,"endColumn":74},{"ruleId":"108","severity":1,"message":"115","line":2,"column":10,"nodeType":"110","messageId":"111","endLine":2,"endColumn":15},{"ruleId":"108","severity":1,"message":"116","line":2,"column":17,"nodeType":"110","messageId":"111","endLine":2,"endColumn":23},{"ruleId":"108","severity":1,"message":"117","line":5,"column":36,"nodeType":"110","messageId":"111","endLine":5,"endColumn":48},{"ruleId":"108","severity":1,"message":"118","line":8,"column":8,"nodeType":"110","messageId":"111","endLine":8,"endColumn":17},{"ruleId":"104","severity":1,"message":"119","line":17,"column":8,"nodeType":"106","endLine":17,"endColumn":10,"suggestions":"120"},{"ruleId":"104","severity":1,"message":"121","line":14,"column":8,"nodeType":"106","endLine":14,"endColumn":10,"suggestions":"122"},"no-native-reassign",["123"],"no-negated-in-lhs",["124"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'computerId' and 'id'. Either include them or remove the dependency array.","ArrayExpression",["125"],"no-unused-vars","'computerId' is assigned a value but never used.